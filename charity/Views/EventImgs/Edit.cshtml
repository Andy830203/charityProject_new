@model charity.Models.EventImg

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>EventImg</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="EId" class="control-label"></label>
                <select asp-for="EId" class="form-control" asp-items="ViewBag.EId"></select>
                <span asp-validation-for="EId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ImgName" class="control-label"></label>
                <input asp-for="ImgName" class="form-control" />
                <span asp-validation-for="ImgName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="hidden" name="fromList" value="@ViewBag.FromList"/>
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a href="@ViewBag.FromList">Back to List</a>
</div>

