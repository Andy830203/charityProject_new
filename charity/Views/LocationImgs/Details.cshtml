@model charity.Models.LocationImg

@{
    ViewData["Title"] = "Details";
}

<h1>地點圖片細節</h1>

<div>
    <h4>地點圖片</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ImgName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ImgName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.LIdNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LIdNavigation.Id)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">編輯</a> |
    <a asp-action="Index">返回地點圖片列表</a>
</div>
