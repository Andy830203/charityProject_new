@model charity.Models.LocationImg

@{
    ViewData["Title"] = "Delete";
}

<h1>刪除</h1>

<h3>是否確認刪除?</h3>
<div>
    <h4>地點圖片</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ImgName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ImgName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.LIdNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LIdNavigation.Id)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="刪除" class="btn btn-danger" /> |
        <a asp-action="Index">返回地點圖片列表</a>
    </form>
</div>
